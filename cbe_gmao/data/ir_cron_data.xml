<odoo>
    <data noupdate="1">
        <record id="ir_cron_maintenance_stock_move" model="ir.cron">
            <field name="name">Automated Stock Move for Maintenance</field>
            <field name="model_id" ref="model_maintenance_request"/>
            <field name="state">code</field>
            <field name="code">model.create_stock_move_for_maintenance()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
